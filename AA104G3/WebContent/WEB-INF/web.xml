<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>AA104G3</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  
  <resource-ref>
    <description>DB Connection</description>
    <res-ref-name>jdbc/TestDB</res-ref-name>
    <res-type>javax.sql.DataSource</res-type>
    <res-auth>Container</res-auth>
  </resource-ref>
  
  <servlet>
  	<servlet-name>ReservationServlet</servlet-name>
  	<servlet-class>com.reservation.controller.ReservationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ReservationServlet</servlet-name>
  	<url-pattern>/reservation/reservation.do</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>Store_CommentServlet</servlet-name>
  	<servlet-class>com.store.controller.Store_CommentServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Store_CommentServlet</servlet-name>
  	<url-pattern>/store_comment/store_comment.do</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>StoreServlet</servlet-name>
  	<servlet-class>com.store.controller.StoreServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>StoreServlet</servlet-name>
  	<url-pattern>/store/store.do</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>StotableServlet</servlet-name>
  	<servlet-class>com.stotable.controller.StotableServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>StotableServlet</servlet-name>
  	<url-pattern>/stotable/stotable.do</url-pattern>
  </servlet-mapping>
  
</web-app>